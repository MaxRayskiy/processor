in

call factorial
jmp return

stop_rec:
push 1
pop eax
ret

factorial:
pop edx // pc
pop ecx  // cnt
mov ebx ecx
push ecx
push edx
cmp ebx 1
jle stop_rec
pop edx
pop ecx
mov ebx ecx
sub ebx 1
push edx  //pc
push ecx
push ebx
call factorial
pop eax
pop ecx
mul eax ecx
pop ebx
push eax
push ebx
ret

return:
out